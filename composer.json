{
  "name": "apsis/one",
  "description": "APSIS One integration for PrestaShop",
  "homepage": "https://github.com/ApsisInternational/apsis_one",
  "type": "prestashop-module",
  "version": "1.0.0",
  "license": "MPL-2.0",
  "authors": [
    {
      "name": "Adeel Qamar",
      "email": "adeel.qamar@apsis.com"
    },
    {
      "name": "APSIS Integration Team"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "prestashop/module-lib-service-container": ">=1.3.1",
    "giggsey/libphonenumber-for-php": ">=8.12.4"
  },
  "autoload": {
    "psr-4": {
      "Apsis\\One\\": "src/"
    },
    "classmap": ["apsis_one.php"],
    "exclude-from-classmap": []
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "prepend-autoloader": false
  }
}
